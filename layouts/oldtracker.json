{
	"shared_item_grid": {
		"type": "array",
		"orientation": "horizontal",
		"margin": "0,0",
		"content": [
			{
				"type": "array",
				"orientation": "horizontal",
				"margin": "0,0",
				"content": [
					{
						"type": "itemgrid",
						"item_margin": "1",
						"h_alignment": "left",
						"item_size": 50,
						"rows": [
							[ "uportalGun", "potatOS", "weightedcube", "lasercube", "antiquecube", "frankencube", "ballcube", "button", "antiquebutton" ],
							[ "gels", "faithplate", "weightedfloorbutton", "antiquefloorbutton", "ballfloorbutton", "laseremitter", "lasercatcher", "laserrelay", "funnel" ],
							[ "lightbridge", "turret", "spacecore", "advcore", "factcore" ]
						]
					}
				]
			}
		]
	},
	"tracker_default": {
		"type": "container",
		"background": "#000000",
		"content": {
			"type": "dock",
			"dropshadow": true,
			"content": [
				{
					"type": "dock",
					"dock": "left",
					"v_alignment": "stretch",
					"content": [
						{
							"type": "group",
							"header": "Items",
							"dock": "top",
							"content": {
								"type": "layout",
								"key": "shared_item_grid"
							}
						}
					]
				},
				{
					"type": "tabbed",
					"tabs": [
						{
							"title": "Both",
							"content": {
								"type": "map",
								"maps": [
									"fullmap",
									"doors"
								]
							}
						},
						{
							"title": "Map",
							"content": {
								"type": "map",
								"maps": [
									"fullmap"
								]
							}
						},
						{
							"title": "Metro",
							"content": {
								"type": "map",
								"maps": [
									"doors"
								]
							}
						},
						{
							"title": "Audio Logs",
							"content": {
								"type": "map",
								"maps": [
									"audioLogs"
								]
							}
						}
					]
				}
			]
		}
	},
	"tracker_horizontal": {
		"type": "container",
		"background": "#000000",
		"content": {
			"type": "dock",
			"dropshadow": true,
			"content": [
				{
					"type": "dock",
					"dock": "bottom",
					"content": [
						{
							"type": "group",
							"header": "Items",
							"dock": "left",
							"margin": "0,0,3,0",
							"content": {
								"type": "layout",
								"h_alignment": "center",
								"v_alignment": "center",
								"key": "shared_item_grid"
							}
						}
					]
				},
				{
					"type": "tabbed",
					"tabs": [
						{
							"title": "Both",
							"content": {
								"type": "map",
								"maps": [
									"fullmap",
									"doors"
								]
							}
						},
						{
							"title": "Map",
							"content": {
								"type": "map",
								"maps": [
									"fullmap"
								]
							}
						},
						{
							"title": "Metro",
							"content": {
								"type": "map",
								"maps": [
									"doors"
								]
							}
						},
						{
							"title": "Audio Logs",
							"content": {
								"type": "map",
								"maps": [
									"audioLogs"
								]
							}
						},
						{
							"title": "Easter Eggs",
							"content": {
								"type": "map",
								"maps": [
									"easterEggs"
								]
							}
						}
					]
				}
			]
		}
	}
}
