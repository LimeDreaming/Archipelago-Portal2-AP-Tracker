{
  "layouts": {
    "tracker_default": {
      "type": "container",
      "background": "#2D2D2D",
      "content": {
        "type": "dock",
        "dropshadow": true,
        "content": [
          {
            "type": "dock",
            "content": {
              "type": "layout",
              "key": "gamemaps"
            }
          },
          {
            "type": "dock",
            "dock": "bottom",
            "content": [
              {
                "type": "dock",
                "dock": "top",
                "v_alignment": "stretch",
                "content": [
                  {
                    "type": "group",
                    "header": "Items",
                    "dock": "left",
                    "background": "#2D2D2D",
                    "content": {
                      "type": "layout",
                      "h_alignment": "center",
                      "v_alignment": "center",
                      "key": "portal_items"
                    }
                  },
                  {
                    "type": "group",
                    "header": "Events",
                    "dock": "left",
                    "margin": "0,0,3,0",
                    "content": {
                      "type": "layout",
                      "h_alignment": "center",
                      "v_alignment": "center",
                      "key": "event_grid"
                    }
                  },
                  {
                    "type": "group",
                    "dock": "right",
                    "margin": "0,0,3,0",
                    "content": {
                      "type": "layout",
                      "h_alignment": "center",
                      "v_alignment": "center",
                      "key": "tab_switch_grid"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "settings_popup": {
      "type": "array",
      "orientation": "horizontal",
      "h_alignment": "left",
      "content": [
        {
          "type": "group",
          "header": "Settings",
          "dock": "bottom",
          "content": {
            "type": "layout",
            "key": "setting_grid"
          }
        }
      ]
    }
  }
}